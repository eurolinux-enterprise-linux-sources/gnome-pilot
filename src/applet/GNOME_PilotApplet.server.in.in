<oaf_info>

<oaf_server iid="OAFIID:GNOME_PilotApplet_Factory"
            type="exe"
            location="@LIBEXECDIR@/gpilot-applet">

        <oaf_attribute name="repo_ids" type="stringv">
                <item value="IDL:Bonobo/GenericFactory:1.0"/>
                <item value="IDL:Bonobo/Unknown:1.0"/>
        </oaf_attribute>
        <oaf_attribute name="name" type="string" value="PilotApplet Factory"/>

</oaf_server>

<oaf_server iid="OAFIID:GNOME_PilotApplet"
            type="factory"
            location="OAFIID:GNOME_PilotApplet_Factory">

        <oaf_attribute name="repo_ids" type="stringv">
                <item value="IDL:GNOME/Vertigo/PanelAppletShell:1.0"/>
                <item value="IDL:Bonobo/Control:1.0"/>
                <item value="IDL:Bonobo/Unknown:1.0"/>
        </oaf_attribute>

        <oaf_attribute name="name" type="string" _value="Pilot Applet"/>
	<oaf_attribute name="description" type="string" _value="Synchronize your data on PalmOS devices"/>

        <oaf_attribute name="panel:category" type="string" _value="Utility"/>
        <oaf_attribute name="panel:icon" type="string" value="palm-pilot-sync.png"/>

</oaf_server>

</oaf_info>
